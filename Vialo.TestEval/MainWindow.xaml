<Window x:Class="Vialo.TestEval.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vialo.TestEval"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <WrapPanel Name="container" HorizontalAlignment="Left" Margin="10" VerticalAlignment="Top"/>
        <WrapPanel Name="result_list" Orientation="Horizontal" HorizontalAlignment="Left" Margin="10" VerticalAlignment="Top">
            
        </WrapPanel>
        <StackPanel Orientation="Vertical" Margin="10" VerticalAlignment="Bottom" HorizontalAlignment="Right">
            <Button Content="Eval" Width="100" Click="Button_Click" Margin="5"/>
            <Button Content="Calc" Width="100" Click="Button_Click_1" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
